package edu.kh.project.myPage.model.service;

import edu.kh.project.member.model.dto.Member;

public interface MyPageService {

	/** 회원 정보 수정 서비스
	 * @param member
	 * @param memberAddress
	 * @return 중복 1 / 아님 0
	 */
	int updateInfo(Member member, String[] memberAddress);

	/** 수정 정보 재조회
	 * @param member
	 * @return
	 */
	Member resrch(Member member);

}
